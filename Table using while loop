///////Program to print the table of any number///////
n=int(input("Enter any number"))
i=1
i<=10
while (i <= 10):
	a = i * n
	print(a)
	i+= 1
//////Program to print the table in the form of 2x1=1///////
i=1
n=int(input("Enter any number:- "))
while(i<=10):
  a=i*n
  print(n ,"x", i, "=", a)
  i+=1
